@model int[,]

<h2>Arreglo Bidimensional</h2>
<table>
    @for (int i = 0; i < Model.GetLength(0); i++)
    {
        <tr>
            @for (int j = 0; j < Model.GetLength(1); j++)
            {
                <td>@Model[i, j]</td>
            }
        </tr>
    }
</table>
<p>
    <a asp-action="Unidimensional">Unidimensional</a>
</p>
